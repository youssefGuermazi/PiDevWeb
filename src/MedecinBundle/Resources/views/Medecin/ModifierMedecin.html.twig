{% extends '@Club/Default/layoutBack.html.twig' %}
{% block body %}
    <!--sidebar end-->
    <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
    <!--main content start-->

        <section class="wrapper site-min-height">
            <h3><i class="fa fa-angle-right"></i> Modifier Medecin</h3>
            {{form_start (form)}}
            <div class="row mt">
                <div class="col-lg-12">
                    <div class="form-panel">
                        <div class="form">

                            <div class="form-group ">

                                <label for="nom" class="control-label col-lg-2">Nom:</label>
                     {{form_widget (form.nom, {'attr': {'class': 'form-control'}})}} <br>
                            </div>
                            <div class="form-group ">

                                <label for="prenom" class="control-label col-lg-2">Prenom:</label>
                    {{form_widget (form.prenom, {'attr': {'class': 'form-control'}})}} <br>
                            </div>
                            <div class="form-group ">

                                <label for="specialite" class="control-label col-lg-2">Spécialité:</label>
                   {{form_widget (form.specialite, {'attr': {'class': 'form-control'}})}} <br>
                            </div>
                            <div class="form-group ">

                                <label for="image" class="control-label col-lg-2">Image:</label>
                    {{form_widget (form.file, {'attr': {'class': 'form-control'}})}}<br>
                            </div>
                    <input type = "submit" value = "Modifier" class = "btn btn-theme" /><br>









                        </div>
                    </div>
                </div>
            </div>
            {{form_end (form) }}

        </section>
        <!-- /wrapper -->

    <!-- /MAIN CONTENT -->
    <!--main content end-->
    <!--footer start-->

    <!--footer end-->

<!-- js placed at the end of the document so the pages load faster -->


<script src="lib/jquery-ui-1.9.2.custom.min.js"></script>
<script src="lib/jquery.ui.touch-punch.min.js"></script>





{% endblock %}






























